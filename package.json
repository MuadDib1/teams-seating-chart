{
  "name": "teams-seating-chart",
  "productName": "teams-seating-chart",
  "version": "1.0.6",
  "description": "seating chart with Teams",
  "main": ".webpack/main",
  "scripts": {
    "start": "chcp 65001 && electron-forge start",
    "test": "jest",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "Naotaka Nakanishi",
    "email": "nakanishi-naotaka@osscat.net"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./src/assets/icon.ico"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "teams_seating_chart",
            "certificateFile": "./private/osscatnet.pfx",
            "certificatePassword": ""
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.js",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "@electron-forge/plugin-webpack": "6.0.0-beta.63",
    "@vercel/webpack-asset-relocator-loader": "1.7.0",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.0.0",
    "electron": "17.0.0",
    "jest": "^27.5.1",
    "node-loader": "^2.0.0",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.0.0",
    "vue-loader": "^15.9.8",
    "vue-template-compiler": "^2.6.14"
  },
  "dependencies": {
    "@mdi/font": "^6.5.95",
    "buefy": "^0.9.19",
    "electron-preferences": "^2.6.0",
    "electron-squirrel-startup": "^1.0.0",
    "konva": "^8.3.3",
    "vue": "^2.6.14",
    "vue-konva": "^2.1.7"
  }
}
